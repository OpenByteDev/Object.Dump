<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>ObjectDump</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>OpenByte</Authors>
    <PackageLicenseExpression>Unlicense</PackageLicenseExpression>
    <Description>Provides simple .Dump() methods to easily print objects in full to the console.</Description>
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/OpenByteDev/Object.Dump</PackageProjectUrl>
    <RepositoryUrl>https://github.com/OpenByteDev/Object.Dump</RepositoryUrl>
    <PackageTags>Dump,Text,PrettyPrint,Console,Logging</PackageTags>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <Optimize>True</Optimize>
  </PropertyGroup>

</Project>
